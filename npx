#!/bin/bash
echo "début script"

# Ensure PREFIX is set, otherwise use the current directory
PREFIX=${PREFIX:-$(pwd)}
echo "commande executée /home/kasia/.nvm/versions/node/v18.20.6/bin/npx "$@""
pushd
cd playwright-sample-project/n1/n2 && ls
# Modify the npx command to always include --prefix
command /home/kasia/.nvm/versions/node/v18.20.6/bin/npx "$@"
popd
echo "fin script"
